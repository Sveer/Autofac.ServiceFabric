  <PropertyGroup>
    <NoWarn>$(NoWarn);NU1605</NoWarn>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.1.0</Version>
    <RepositoryUrl>http://nuget.cloud.pryaniky.ru</RepositoryUrl>
    <Description>Autofac.ServicaFabric for Pryaniky (NetStandard 2.0)</Description>
    <Company>Autofac</Company>
    <Product>Autofac ServiceFabric</Product>
  </PropertyGroup>
